{
	"compilerOptions": {
		"declaration": true,
		"strict": true,
		"module": "node16",
		"moduleResolution": "node16",
		"moduleDetection": "force",
		"target": "ES2022",
		"resolveJsonModule": false,
    "skipLibCheck": true
	},
